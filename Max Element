#include<stdio.h>
int main(){
    int n=3;
    int N;
int arr[n];
  
    int max=arr[0];
   
    printf("Element number :");
    scanf("%d",&n);
    
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    
     
    
    for(int i=1;i<n;i++){
      if(arr[i]<max){
       max=arr[i];
      } 
      
    }
    
      printf("Enter the maximum number is :%d\n",max);
    }
